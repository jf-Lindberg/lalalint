{
  "commands": {
    "check": {
      "lint": true,
      "showerrors": true
    },
    "print": {
      "lint": false
    },
    "write": {
      "lint": true
    }
  },
  "global": {
    "showerrors": true
  },
  "rules": {
    "indentenvironments": {
      "enabled": true,
      "indent": 1
    },
    "spaceaftercomments": {
      "enabled": true,
      "symbol": "%"
    }
  }
}