{
  "global": {
    "showErrors": false
  },
  "commands": {
    "print": {
      "lint": false
    },
    "write": {
      "lint": true
    },
    "check": {
      "lint": true,
      "showErrors": true
    }
  },
  "rules": {
    "spaceAfterComments": {
      "enabled": true,
      "symbol": "%"
    },
    "indentEnvironments": {
      "enabled": true,
      "indent": 1
    }
  }
}
